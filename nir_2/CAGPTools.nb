Cell[BoxData[
    \(update[a : automatonPattern, positionindependenttransformationrules_, 
        positiondependenttransformationrules_] \
:= \[IndentingNewLine]MapIndexed[
        combo[ReplaceRepeated[#1[\([\)\(2, 1\)\(]\)], 
              Join[positionindependenttransformationrules, 
                positiondependenttransformationrules[#1, #2]]], #1] &, 
        Partition[
          getRing[a], {3, 2}, {1, 
            1}, {{2, 1}, {\(-2\), 2}}], {2}]\)], "NumberedInput",
  CellTags->{"keeper", "update"}],

